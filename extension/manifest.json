{
  "manifest_version": 3,
  "name": "WooCommerce Scraper",
  "version": "1.0",
  "description": "Scrape WooCommerce product data",
  "permissions": [
    "storage",
    "activeTab",
    "identity"
  ],
  "host_permissions": [
    "https://panel-production-5838.up.railway.app/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
